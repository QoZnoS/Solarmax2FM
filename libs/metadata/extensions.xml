    <!-- id：与关卡集绑定 -->
    <!-- teamCount：势力总数 -->
    <!-- describe：关卡集描述，因为没有中文字体，描述和名称都只能用英文 -->
    <!-- name：关卡集名称 -->
<!-- level示例<level id="1" difficult="hard" nodeCount="3"> -->
    <!-- id：关卡数 -->
    <!-- difficult：难度选项，默认对所有难度生效，三个难度分别为 eazy normal hard -->
    <!-- nodeCount：规定开局生成天体的天体个数 -->
<!-- node示例<node tag="0" x="512" y="384" team="0" size="0.3" type="0" startVal="0" popVal="30" attackType="0" attackRange="0" attackRate="0" attackLast="0" buildRate="0.6" orbitNode="-1" orbitDist="0" orbitSpeed="0.1" hp="0" hpMult="1" captureTeam="0"/> -->
    <!-- tag：天体标识 -->
    <!-- x，y：坐标 -->
    <!-- team：势力 -->
    <!-- size：大小 -->
    <!-- type：类型：0星球 1传送门 2造船厂 3障碍 4炮塔 5星核 6太空堡垒 7脉冲 8黑洞 9航母 -->
    <!-- startVal：初始人口，可填数组，若为障碍则决定障碍线 -->
    <!-- popVal：提供人口上限 -->
    <!-- attackType：攻击类型，0不攻击，1炮塔类，2脉冲，3黑洞，4航母 -->
    <!-- attackRange：攻击范围，炮塔堡垒为180，黑洞为150，单位px -->
    <!-- attackRate：攻击间隔 -->
    <!-- attackLast：攻击持续时间 -->
    <!-- buildRate：生产速度（船/秒） -->
    <!-- orbitNode：轨道中心天体tag -->
    <!-- orbitDist：轨道半径 -->
    <!-- orbitSpeed：轨道速度 -->
    <!-- hp：初始占领度 -->
    <!-- hpMult：占领难度倍率 -->
    <!-- captureTeam：初始占领度势力 -->
